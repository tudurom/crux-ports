# Description: Highly customizable and functional web browser
# URL:         https://pwmt.org/projects/jumanji
# Packager:    John Vogel, jvogel4 at stny dot rr dot com
# Maintainer:  tudurom, xenogenesis@openmailbox.org
# Depends on:  girara webkitgtk

name=jumanji
version=de4c6c3c
release=1
source=(https://git.pwmt.org/pwmt/$name/repository/archive.tar.gz\?ref=$version)

build() {
	LOCAL_FILENAME=$(get_filename ${source[0]})
	bsdtar -p -o -C $SRC -xf $LOCAL_FILENAME
	cd $name-${version}*
	make
	make DESTDIR=$PKG install
}
